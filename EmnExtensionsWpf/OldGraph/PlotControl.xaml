<UserControl x:Class="EmnExtensions.Wpf.Plot.PlotControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:emnExt="clr-namespace:EmnExtensions.Wpf"
    IsHitTestVisible="False"
			 >
	<Grid>
		<emnExt:TickedAxis x:Name="tickedAxisBot" AxisPos="BelowGraph"/>
		<emnExt:TickedAxis x:Name="tickedAxisLft" AxisPos="LeftOfGraph"/>
		<emnExt:TickedAxis x:Name="tickedAxisTop" AxisPos="AboveGraph" />
		<emnExt:TickedAxis x:Name="tickedAxisRgt" AxisPos="RightOfGraph"/>
	</Grid>
</UserControl>
