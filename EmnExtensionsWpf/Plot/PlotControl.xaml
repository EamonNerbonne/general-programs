<UserControl x:Class="EmnExtensions.Wpf.Plot.PlotControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:emnExt="clr-namespace:EmnExtensions.Wpf"
    xmlns:emnPlot="clr-namespace:EmnExtensions.Wpf.Plot"
    Height="300" Width="300">
	<Grid Margin="10" Background="#eee"  >
		<emnPlot:TickedAxis AttemptBorderTicks="True" x:Name="tickedAxisBot" AxisPos="BelowGraph"   DataBound="10,70" DataMargin="1" Background="#2f0f"  />
		<emnPlot:TickedAxis AttemptBorderTicks="True" x:Name="tickedAxisLft" AxisPos="LeftOfGraph"  DataBound="0,1"  DataMargin="1"  Background="#20ff"/>
		<emnPlot:TickedAxis AttemptBorderTicks="True" x:Name="tickedAxisTop" AxisPos="AboveGraph"   DataBound="2.0,2.000000000001"  DataMargin="1"  Background="#20f0"/>
		<emnPlot:TickedAxis AttemptBorderTicks="True" x:Name="tickedAxisRgt" AxisPos="RightOfGraph" DataBound="0,8.5"  DataMargin="1"  Background="#2ff0"/>
	</Grid>
</UserControl>
