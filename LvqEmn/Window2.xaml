<Window x:Class="LVQeamon.Window2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:emn="clr-namespace:EmnExtensions.Wpf;assembly=EmnExtensionsWpf"
	xmlns:emnPlot="clr-namespace:EmnExtensions.Wpf.Plot;assembly=EmnExtensionsWpf"
    Title="Window2" Height="450" Width="600">
    <Grid Margin="10" Background="#eee"  >
		<emnPlot:TickedAxis AttemptBorderTicks="True"  Name="tickedAxis" DataUnits="cm" SnapTo="Top" VerticalAlignment="Bottom"  DataBound="10,999.5" DataMargin="1" Background="#20Ff00Ff"  />
		<emnPlot:TickedAxis AttemptBorderTicks="True" Name="tickedAxisL" DataUnits="cm" SnapTo="Right" HorizontalAlignment="Left"  DataBound="0.5,1.9"  DataMargin="1"  Background="#2000FfFf"/>
	</Grid>
</Window>
