<Window x:Class="RealSimilarityMds.MusicMdsDisplay"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:emnExt="clr-namespace:EmnExtensions.WPF;assembly=EmnExtensionsWpf"
    Title="MusicMdsDisplay" Height="600" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="500*" />
            <ColumnDefinition Width="5" />
            <ColumnDefinition Width="300*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="929*" />
            <RowDefinition Height="33" />
        </Grid.RowDefinitions>
        <ProgressBar Name="progressBar" Margin="4,4,4,4" Grid.Row="1"  />
        <Viewbox Name="pointViewbox" Stretch="Uniform" Margin="4,4,4,4" >
            <Canvas Name="pointCanvas"></Canvas>
        </Viewbox>
        <emnExt:LogControl Name="logControl" ClaimStandardError="true" ClaimStandardOut="true" Grid.Column="2" TextWrapping="Wrap" />
        <GridSplitter Grid.Column="1" HorizontalAlignment="Stretch" Name="gridSplitter1" Grid.RowSpan="2"  />
        <Label Grid.Column="2" Grid.Row="1" Margin="4,4,4,4" Name="labelETA">Label</Label>
    </Grid>
</Window>
